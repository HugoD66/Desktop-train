<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" stylesheets="@assets/style.css" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.gestionbudget.dialog.DialogController">
    <VBox alignment="CENTER" spacing="20.0">
        <TextField fx:id="periode" promptText="Période" />
        <TextField fx:id="total" promptText="Total" />
        <TextField fx:id="logement" promptText="Logement" />
        <TextField fx:id="nourriture" promptText="Nourriture" />
        <TextField fx:id="sorties" promptText="Sorties" />
        <TextField fx:id="transport" promptText="Transport" />
        <TextField fx:id="voyage" promptText="Voyage" />
        <TextField fx:id="impots" promptText="Impôts" />
        <TextField fx:id="autres" promptText="Autres" />
    </VBox>

    <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#onCancel" text="Cancel" />
    <Button fx:id="btnOk" mnemonicParsing="false" onAction="#onSubmit" text="Ok" />
</VBox>